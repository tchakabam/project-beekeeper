{
  "name": "beekeepr",
  "description": "Beekeepr P2P live stream",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "author": "Stephan Hesse <disparat@gmail.com> & Novage LLC",
  "homepage": "https://github.com/tchakabam/project-beekeeper",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "p2p",
    "peer-to-peer",
    "hls",
    "dash",
    "webrtc",
    "video",
    "mse",
    "player",
    "torrent",
    "bittorrent",
    "webtorrent",
    "hlsjs",
    "shaka player"
  ],
  "scripts": {
    "build": "npm run webpack:build",
    "postinstall": "git submodule update --init",
    "start": "npm run start:node",
    "start:webdev": "npm run webpack:devserver",
    "start:node": "DEBUG=bk:* ts-node ./bin/node-agent.ts",
    "start:herd": "ts-node ./bin/agent-herd.ts",
    "test": "jest",
    "lint": "eslint --ext .ts --ext .js .",
    "webpack:build": "rimraf ./dist && webpack --progress",
    "webpack:devserver": "webpack-dev-server --progress --port 8080",
    "webpack:watch": "rimraf ./dist && webpack --progress --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tchakabam/project-beekeeper"
  },
  "dependencies": {
